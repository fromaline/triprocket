{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "linebreak-style": "off",
    "arrow-parens": 0,
    "require-jsdoc": "off",
    "object-curly-spacing": "off",
    "no-trailing-spaces": "off",
    "no-debugger": "off",
    "indent": "off",
    "operator-linebreak": "off",
    "max-len": "off",
    "no-invalid-this": "off",
    "eol-last": "off"
  },
  "extends": ["eslint:recommended", "google"]
}